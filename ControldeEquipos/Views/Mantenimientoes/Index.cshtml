@model IEnumerable<Mantenimiento>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Equipo_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Fecha_Mantenimiento)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Descripcion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tecnico)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Equipo_ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha_Mantenimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Descripcion)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tecnico)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ID_Mantenimiento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ID_Mantenimiento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ID_Mantenimiento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
